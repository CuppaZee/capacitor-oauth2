{
    "name": "@byteowls/capacitor-oauth2",
    "version": "3.0.0",
    "description": "Capacitor OAuth 2 client plugin",
    "author": "Michael Oberwasserlechner",
    "homepage": "https://github.com/capacitor-community/oauth2",
    "license": "MIT",
    "main": "dist/esm/index.js",
    "types": "dist/esm/index.d.ts",
    "scripts": {
        "build": "npm run clean && tsc",
        "clean": "rm -rf ./dist",
        "watch": "tsc --watch",
        "test": "jest",
        "removePacked": "rm -f capacitor-oauth2-*.tgz",
        "publishLocally": "npm run removePacked && npm run build && npm pack",
        "prepublishOnly": "npm run build"
    },
    "files": [
        "dist/",
        "ios/",
        "android/",
        "ByteowlsCapacitorOauth2.podspec"
    ],
    "keywords": [
        "capacitor",
        "capacitor-plugin",
        "oauth2",
        "oauth2-client",
        "social-login"
    ],
    "capacitor": {
        "ios": {
            "src": "ios"
        },
        "android": {
            "src": "android"
        }
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/capacitor-community/oauth2"
    },
    "bugs": {
        "url": "https://github.com/capacitor-community/oauth2/issues"
    },
    "publishConfig": {
        "access": "public"
    },
    "peerDependencies": {
        "@capacitor/core": "^3.0.0"
    },
    "dependencies": {},
    "devDependencies": {
        "@capacitor/android": "^3.0.0",
        "@capacitor/core": "^3.0.0",
        "@capacitor/ios": "^3.0.0",
        "@types/jest": "26.0.20",
        "jest": "26.6.3",
        "ts-jest": "26.5.1",
        "typescript": "3.8.3"
    }
}
